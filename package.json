{
  "name": "group-tabs",
  "displayName": "group tabs",
  "description": "",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.60.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "*"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "fileGroups.helloWorld",
        "title": "Hello World"
      },
      {
        "command": "fileGroups.say",
        "title": "cowsay say"
      },
      {
        "command": "fileGroups.save",
        "title": "Save all tabs"
      },
      {
        "command": "fileGroups.deleteGroup",
        "title": "Delete group",
        "icon": {
          "light": "resources/light/delete.svg",
          "dark": "resources/dark/delete.svg"
        }
      },
      {
        "command": "fileGroups.refreshEntry",
        "title": "Refresh",
        "icon": {
          "light": "resources/light/refresh.svg",
          "dark": "resources/dark/refresh.svg"
        }
      },
      {
        "command": "fileGroups.addEntry",
        "title": "Add current files"
      },
      {
        "command": "fileGroups.editEntry",
        "title": "Edit",
        "icon": {
          "light": "resources/light/edit.svg",
          "dark": "resources/dark/edit.svg"
        }
      },
      {
        "command": "fileGroups.deleteEntry",
        "title": "Delete",
        "icon": {
          "light": "resources/light/delete.svg",
          "dark": "resources/dark/delete.svg"
        }
      }
    ],
    "menus": {
      "view/title": [
        {
          "command": "fileGroups.refreshEntry",
          "when": "view == fileGroups",
          "group": "navigation"
        },
        {
          "command": "fileGroups.addEntry",
          "when": "view == fileGroups"
        }
      ],
      "view/item/context": [
        {
          "command": "fileGroups.deleteEntry",
          "when": "view == fileGroups && viewItem == tab",
          "group": "inline"
        },
        {
          "command": "fileGroups.editEntry",
          "when": "view == fileGroups && viewItem == tab",
          "group": "inline"
        },
        {
          "command": "fileGroups.deleteGroup",
          "when": "view == fileGroups && viewItem == group",
          "group": "inline"
        }
      ]
    },
    "viewsContainers": {
      "activitybar": [
        {
          "id": "groups-explorer",
          "title": "Groups Explorer",
          "icon": "resources/tab.svg"
        }
      ]
    },
    "views": {
      "groups-explorer": [
        {
          "id": "fileGroups",
          "name": "Groups",
          "icon": "resources/tab.svg",
          "contextualTitle": "Groups Explorer"
        }
      ]
    },
    "viewsWelcome": [
      {
        "view": "fileGroups",
        "contents": "No groups found in this workspace.\n[Add open files](command:fileGroups.addEntry)"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "test-compile": "tsc -p ./",
    "test-watch": "tsc -watch -p ./",
    "pretest": "npm run test-compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.60.0",
    "@types/glob": "^7.1.3",
    "@types/mocha": "^8.2.2",
    "@types/node": "14.x",
    "eslint": "^7.27.0",
    "@typescript-eslint/eslint-plugin": "^4.26.0",
    "@typescript-eslint/parser": "^4.26.0",
    "glob": "^7.1.7",
    "mocha": "^8.4.0",
    "typescript": "^4.3.2",
    "vscode-test": "^1.5.2",
    "ts-loader": "^9.2.2",
    "webpack": "^5.38.1",
    "webpack-cli": "^4.7.0"
  }
}
